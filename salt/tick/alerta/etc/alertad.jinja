{%- set ip4_interfaces = grains['ip4_interfaces'] -%}
DEBUG = True
BASE_URL = ''
LOGGER_NAME = 'alerta'
LOG_FILE = None

CORS_ORIGINS = [
  'http://{{ grains.fqdn }}',
{% for interface, addrs in ip4_interfaces.items() %}
  {% if addrs[0] != '127.0.0.1' %}
  'http://{{addrs[0]}}',
	{% endif %}
{% endfor %}
]
